{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "yarn build && ncc build --source-map --license licenses.txt",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "^0.5.1",
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@slack/webhook": "^6.0.0",
    "axios": "^0.21.1",
    "husky": "^6.0.0",
    "mime": "^2.5.2",
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "puppeteer-core": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/mime": "^2.0.3",
    "@types/node": "^14.14.9",
    "@types/pixelmatch": "^5.2.3",
    "@types/pngjs": "^6.0.0",
    "@types/resemblejs": "^3.2.0",
    "@typescript-eslint/parser": "^4.8.1",
    "@vercel/ncc": "^0.28.3",
    "eslint": "^7.17.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^27.0.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.1.0",
    "prettier": "2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
